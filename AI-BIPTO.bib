@misc{Wilbur2022,
  title = {An {{Online Approach}} to {{Solve}} the {{Dynamic Vehicle Routing Problem}} with {{Stochastic Trip Requests}} for {{Paratransit Services}}},
  author = {Wilbur, Michael and Kadir, Salah Uddin and Kim, Youngseo and Pettet, Geoffrey and Mukhopadhyay, Ayan and Pugliese, Philip and Samaranayake, Samitha and Laszka, Aron and Dubey, Abhishek},
